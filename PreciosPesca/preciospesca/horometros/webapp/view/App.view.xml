<mvc:View
    controllerName="tasa.com.horometros.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
	xmlns:ui="sap.ui.table"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content />
					<ui:Table 
                            rows="{PoliticaPrecio>/listaPolitica}"
                            visibleRowCount="7"
                            selectionMode="MultiToggle"
                           >
				<ui:columns>
                 
                	<ui:Column
						id="CDPPC"
						width="11rem"
						sortProperty="CDPPC"
						filterProperty="CDPPC"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
                        <ui:multiLabels>
							<Label visible="false" />
							<Label text="Codigo de Precio" />
						</ui:multiLabels>
						
						<ui:template>
							<Text text="asdasd" wrapping="false" />
						</ui:template>
					</ui:Column>
				</ui:columns>
			
					</ui:Table>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
