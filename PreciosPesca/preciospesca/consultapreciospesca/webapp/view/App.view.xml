					</Column>
					 <Column
						id="BONIF"
						width="11rem"
						sortProperty="BONIF"
						filterProperty="BONIF"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Bonif" />
						<template>
							<m:Text text="{Acopio>BONIF}" wrapping="false" />
						</template>
					</Column>
					 <Column
						id="PRVEN"
						width="11rem"
						sortProperty="PRVEN"
						filterProperty="PRVEN"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Venta" />
						<template>
							<m:Text text="{Acopio>PRVEN}" wrapping="false" />
						</template>
					</Column>
					 <Column
						id="DESC_WAERS"
						width="11rem"
						sortProperty="DESC_WAERS"
						filterProperty="DESC_WAERS"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Moneda" />
						<template>
							<m:Text text="{Acopio>DESC_WAERS}" wrapping="false" />
						</template>
					</Column>
					 <Column
						id="DESC_ESPRC"
						width="11rem"
						sortProperty="DESC_ESPRC"
						filterProperty="DESC_ESPRC"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Estado" />
						<template>
							<m:Text text="{Acopio>DESC_ESPRC}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="PCSPP"
						width="11rem"
						sortProperty="PCSPP"
						filterProperty="PCSPP"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="% Prop" />
						<template>
							<m:Text text="{Acopio>PCSPP}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="PCCSG"
						width="11rem"
						sortProperty="PCCSG"
						filterProperty="PCCSG"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="% Def" />
						<template>
							<m:Text text="{Acopio>PCCSG}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="DESC_ESCSG"
						width="11rem"
						sortProperty="DESC_ESCSG"
						filterProperty="DESC_ESCSG"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Estado" />
						<template>
							<m:Text text="{Acopio>DESC_ESCSG}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="OBCDF"
						width="11rem"
						sortProperty="OBCDF"
						filterProperty="OBCDF"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Obs. super." />
						<template>
							<m:Text text="{Acopio>OBCDF}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="OBCPP"
						width="11rem"
						sortProperty="OBCPP"
						filterProperty="OBCPP"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Obs. ger." />
						<template>
							<m:Text text="{Acopio>OBCPP}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="CALIDAD"
						width="11rem"
						sortProperty="CALIDAD"
						filterProperty="CALIDAD"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Calidad" />
						<template>
							<m:Text text="{Acopio>CALIDAD}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="JUVEN"
						width="11rem"
						sortProperty="JUVEN"
						filterProperty="JUVEN"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="% Juveniles" />
						<template>
							<m:Text text="{Acopio>JUVEN}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="TVN"
						width="11rem"
						sortProperty="TVN"
						filterProperty="TVN"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="TVN" />
						<template>
							<m:Text text="{Acopio>TVN}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="TDC"
						width="11rem"
						sortProperty="TDC"
						filterProperty="TDC"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="TDC" />
						<template>
							<m:Text text="{Acopio>TDC}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="OBCPP2"
						width="11rem"
						sortProperty="OBCPP"
						filterProperty="OBCPP"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Obs. calidad" />
						<template>
							<m:Text text="{Acopio>OBCPP}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="EBELN"
						width="11rem"
						sortProperty="EBELN"
						filterProperty="EBELN"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Orden compra" />
						<template>
							<m:Text text="{Acopio>EBELN}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="BELNR"
						width="11rem"
						sortProperty="BELNR"
						filterProperty="BELNR"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Factura" />
						<template>
							<m:Text text="{Acopio>BELNR}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="DOCFI"
						width="11rem"
						sortProperty="DOCFI"
						filterProperty="DOCFI"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Doc. Fi" />
						<template>
							<m:Text text="{Acopio>DOCFI}" wrapping="false" />
						</template>
					</Column>
					<Column
						id="DOCFI2"
						width="11rem"
						sortProperty="DOCFI2"
						filterProperty="DOCFI2"
						showFilterMenuEntry="true"
						showSortMenuEntry="true">
						<m:Label text="Doc. Anulación" />
						<template>
							<m:Text text="{Acopio>DOCFI2}" wrapping="false" />
						</template>
					</Column>
					
					<!--Columnas-->

				</columns>
				<footer>
					<m:OverflowToolbar id="infobar">

                    </m:OverflowToolbar>
				</footer>
			</Table>

        </semantic:content>

	</semantic:SemanticPage>
		</m:App>
	</m:Shell>
</mvc:View>
